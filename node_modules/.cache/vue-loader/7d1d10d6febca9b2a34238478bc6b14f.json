r\">\n    <button @click=\"switchCreateProduct\">Create product</button>\n    <div\n      v-if=\"isCreateProductVisible\"\n      @click=\"switchCreateProduct\"\n      class=\"overlay\"\n    >\n      <div @click.stop class=\"overlay-background\">\n        <h1>Create product:</h1>\n        <input\n          class=\"input\"\n          type=\"text\"\n          v-model=\"name\"\n          placeholder=\"Product name\"\n        />\n        <input\n          class=\"input\"\n          type=\"text\"\n          v-model=\"price\"\n          placeholder=\"Product price\"\n        />\n        <textarea\n          class=\"input input-textarea\"\n          type=\"text\"\n          v-model=\"description\"\n          placeholder=\"Type description\"\n        />\n        <div class=\"input-background\">\n          <input\n            class=\"input\"\n            type=\"text\"\n            v-model=\"location\"\n            placeholder=\"location\"\n          />\n          <img\n            :src=\"LocationImg\"\n            alt=\"#\"\n            class=\"location\"\n            title=\"Get current location\"\n            @click=\"getLocation\"\n          />\n        </div>\n        <div class=\"image-container\">\n          <img class=\"image\" v-if=\"imageUrl\" :src=\"imageUrl\" />\n        </div>\n        <input\n          class=\"input-file\"\n          type=\"file\"\n          name=\"image\"\n          ref=\"file\"\n          accept=\"image/*\"\n          @change=\"pickImage\"\n        />\n        <button @click=\"createProduct\" class=\"button\">Create</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from 'vuex';\nimport LocationSvg from '../assets/svg/location.svg';\nexport default {\n  name: 'CreateProduct',\n  data() {\n    return {\n      isCreateProductVisible: false,\n      name: '',\n      price: '',\n      description: '',\n      location: '',\n      image: null,\n      imageUrl: null,\n      coordinates: null,\n      LocationImg: LocationSvg,\n    };\n  },\n  methods: {\n    ...mapActions(['postProduct']),\n    switchCreateProduct: function () {\n      this.isCreateProductVisible = !this.isCreateProductVisible;\n    },\n    getLocation: function () {\n      if (!('geolocation' in navigator)) return;\n      navigator.geolocation.getCurrentPosition(\n        pos => {\n          this.coordinates = pos;\n          console.log('pos: ', pos);\n        },\n        error => {\n          console.error('getLocation() | Error: ', error);\n        },\n      );\n    },\n    pickImage() {\n      const image = this.$refs.file.files[0];\n      this.image = image;\n      this.imageUrl = URL.createObjectURL(image);\n    },\n    createProduct: function () {\n      const newProduct = {\n        ownerId: this.user.id,\n        name: this.name,\n        price: this.price,\n        description: this.description,\n        location: this.location,\n      };\n      const formData = new FormData();\n      formData.append('file', this.image);\n      this.postProduct(newProduct);\n    },\n  },\n  computed: {\n    ...mapGetters(['user']),\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n$textColor: #314252;\n$whiteColor: #ffffff;\n$linkColor: #28349b;\n$grayColorLight: #eeeeee;\n$grayColor: #e1e1e1;\n$grayColorDark: #757575;\n$greenColor: #3fba83;\n$greenColorLight: #bae6d5;\n$greenColorLight2: #c7ebdf;\n$orangeColor: #dba614;\n$redColor: #c42e1a;\n\n.container {\n  margin: 25px 0 25px 0;\n}\n\n.overlay {\n  transition: 0.5s;\n  position: absolute;\n  height: 100vh;\n  width: 100vw;\n  background-color: rgba(0, 0, 0, 0.5);\n  top: 0;\n  left: 0;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  &-background {\n    width: 400px;\n    background-color: $whiteColor;\n    display: flex;\n    flex-direction: column;\n    justify-content: flex-start;\n    align-items: center;\n  }\n}\n\n.input {\n  height: 35px;\n  width: 80%;\n  box-sizing: border-box;\n  border: none;\n  outline: none;\n  transition: 0.5s;\n  padding-left: 10px;\n  cursor: pointer;\n  margin: 10px 0 10px 0;\n  border: 1px solid $greenColor;\n  background-color: $whiteColor;\n  &-textarea {\n    font-size: 0.7em;\n    resize: none;\n    height: 120px;\n    padding: 5px;\n  }\n  &-background {\n    width: 100%;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    position: relative;\n  }\n}\n\n.location {\n  width: 30px;\n  cursor: pointer;\n  position: absolute;\n  right: 5px;\n}\n\n.button {\n  height: 55px;\n  width: 80%;\n  box-sizing: border-box;\n  border: none;\n  outline: none;\n  transition: 0.5s;\n  background-color: $greenColor;\n  border: 1px solid $greenColor;\n  padding-left: 10px;\n  cursor: pointer;\n  color: $whiteColor;\n  margin: 25px 0 25px 0;\n  &:hover {\n    border: 1px solid $greenColor;\n    color: $greenColor;\n    background-color: $whiteColor;\n  }\n}\n\n.image {\n  width: 200px;\n  height: 200px;\n  &-container {\n  }\n}\n\n.input-file {\n}\n</style>\n"]}]}