->\n      <FormInput fieldName=\"emaEl\" />\n      <div class=\"form-input-container\">\n        <input\n          class=\"\n            form-input-container-background\n            form-input-container-background-input\n          \"\n          type=\"password\"\n          v-model=\"password\"\n          placeholder=\"password\"\n        />\n      </div>\n      <div class=\"form-input-container form-inpit-container-button\">\n        <button\n          class=\"\n            form-input-container-background\n            form-input-container-background-button\n          \"\n          @click.prevent=\"authorization\"\n        >\n          {{ this.formType.toUpperCase() }}\n        </button>\n      </div>\n      <div class=\"form-switch-container\">\n        <div class=\"form-switch\" @click=\"switchFormType\">\n          {{ this.formType === 'registration' ? signInMessage : signUpMessage }}\n        </div>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nimport requests from '../services/requests';\nimport FormInput from './FormInput.vue';\nexport default {\n  name: 'LoginForm',\n  components: {\n    FormInput,\n  },\n  methods: {\n    registration: async function () {\n      await requests\n        .registration({ email: this.email, password: this.password })\n        .then((response) => console.log('response: ', response));\n    },\n    authorization: async function () {\n      await requests\n        .authorization({ email: this.email, password: this.password })\n        .then(\n          (response) =>\n            (document.cookie = `token=${response.data.token}; max-age=3000`),\n        );\n\n      // await requests\n      //   .getWithToken()\n      //   .then((res) => console.log('res log: ', res));\n    },\n    deleteAccount: async function () {\n      // await requests\n      //   .delete({ email: this.email })\n      //   .then((response) => console.log('response: ', response));\n    },\n    switchFormType() {\n      this.formType =\n        this.formType === 'registration' ? 'authorization' : 'registration';\n      console.log('formtype: ', this.formType);\n    },\n  },\n  data() {\n    return {\n      formType: 'registration',\n      email: '',\n      password: '',\n      signInMessage: 'Already have an account? Sign in',\n      signUpMessage: 'Log in to start. Have no account? Sign up',\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n$textColor: #314252;\n$whiteColor: #ffffff;\n$linkColor: #28349b;\n$grayColorLight: #eeeeee;\n$grayColor: #e1e1e1;\n$grayColorDark: #757575;\n$greenColor: #3fba83;\n$greenColorLight: #bae6d5;\n$greenColorLight2: #c7ebdf;\n\n.container {\n  width: 500px;\n  height: 400px;\n  box-shadow: 0 0 4px rgba(0, 0, 0, 0.5);\n  background-color: $grayColorLight;\n}\n\n.form {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n}\n\n.form-input-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  padding: 20px 0 20px 0;\n}\n\n.form-inpit-container-button {\n  padding: 20px 0 0 0;\n}\n\n.form-input-container-background {\n  height: 55px;\n  width: 80%;\n  box-sizing: border-box;\n  border: none;\n  outline: none;\n  transition: 0.5s;\n  &-input {\n    background-color: $grayColorLight;\n    padding-left: 10px;\n    background-color: $grayColor;\n    border-bottom: 1px solid $grayColor;\n    &:focus {\n      border-bottom: 1px solid $greenColor;\n    }\n  }\n  &-button {\n    background-color: $greenColor;\n    border: 1px solid $greenColor;\n    padding-left: 10px;\n    cursor: pointer;\n    color: $whiteColor;\n    &:hover {\n      border: 1px solid $greenColor;\n      color: $greenColor;\n      background-color: $whiteColor;\n    }\n  }\n}\n\n.form-switch-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  padding: 5px 0 5px 0;\n}\n\n.form-switch {\n  font-size: 0.6em;\n  cursor: pointer;\n  transition: 0.5s;\n  &:hover {\n    color: $linkColor;\n  }\n}\n</style>\n"]}]}